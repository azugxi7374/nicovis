{
	"name": "nicocomvis",
	"version": "0.0.1",
	"manifest_version": 2,
	"page_action": {
	},
	"permissions":[
		"cookies",
		"*://*.nicovideo.jp/",
		"webNavigation"
	],
	"content_scripts" : [
		{
			"matches": ["*://*.nicovideo.jp/*"],
			"js" : [
				"jquery-2.1.4.min.js",
				"background.js", 
				"nico_model.js",
				"underscore-min.js"
		],
		"persistent": false
		}
	]
}
